<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="TextBox" x:Key="Ky_Txb_EnDecrypt">
        <Setter Property="FontSize" Value="20" />
        <Setter Property="FontFamily" Value="Consolas" />
        <Setter Property="Foreground" Value="#FFFFFF" />
        <Setter Property="Background" Value="#FF343333" />
        <Setter Property="BorderBrush" Value="#FF00FF" />
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="CaretBrush" Value="#FF00FF" />
        <Setter Property="SelectionBrush" Value="#FF5E146A"/>
    </Style>
    
</ResourceDictionary>